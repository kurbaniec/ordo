    "@types/mocha": "^8.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "mocha": "^8.1.1",
    "ts-node": "^9.0.0",
    "webpack": "^4.44.1",
